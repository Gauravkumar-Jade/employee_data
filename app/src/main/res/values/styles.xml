<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="et_style" >
        <item name="android:imeOptions">actionDone</item>
        <item name="android:fontFamily">@font/dm_sans</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:singleLine">true</item>
        <item name="android:maxLines">1</item>
        <item name="android:minLines">1</item>
        <item name="background">@null</item>
        <item name="android:background">@drawable/bg_edittext</item>
    </style>

    <style name="button_style" parent="Widget.MaterialComponents.Button">
        <item name="android:insetTop">0dp</item>
        <item name="android:insetBottom">0dp</item>
        <item name="textAllCaps">false</item>
        <item name="android:textStyle">bold</item>
        <item name="fontFamily">@font/dm_sans</item>
        <item name="android:textSize">17sp</item>
    </style>

    <style name="txt_16">
        <item name="fontFamily">@font/dm_sans</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
    </style>


</resources>